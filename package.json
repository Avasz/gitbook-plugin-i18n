{
  "name": "gitbook-plugin-nepali-locale-switcher",
  "description": "implement Nepali locale on demand",
  "main": "index.js",
  "version": "1.0.0",
  "dependencies": {
    "lodash": "^3.10.1"
  },
  "engines": {
    "gitbook": ">=2.4.0"
  },
  "homepage": "https://github.com/avasz/gitbook-plugin-nepali-locale-switcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/avasz/gitbook-plugin-nepali-locale-switcher"
  },
  "license": "Apache-2.0",
  "gitbook": {
    "properties": {
      "all": {
        "default": [
          "nepali",
          "english"
        ],
        "items": {
          "type": "string"
        },
        "type": "array",
        "uniqueItems": true
      },
      "EnglishUrl": {
        "title": "english",
        "type": "string",
        "default": "../en_US"
      },
      "NepaliUrl": {
        "title": "nepali",
        "type": "string",
        "default": "../ne_NP"
      }
    }
  },
  "devDependencies": {
    "eslint": "^2.7.0"
  }
}
